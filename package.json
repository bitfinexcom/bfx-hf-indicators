{
  "name": "bfx-hf-indicators",
  "version": "2.0.0",
  "description": "A library of trading indicators for Node.JS",
  "main": "./dist/index.js",
  "directories": {
    "lib": "lib"
  },
  "author": "Bitfinex",
  "contributors": [
    "Cris Mihalache <cris.m@bitfinex.com> (https://www.bitfinex.com)",
    "Paolo Ardoino <paolo@bitfinex.com> (https://www.bitfinex.com)",
    "Jacob Plaster <jacob.plaster@bitfinex.com> (https://www.bitfinex.com)"
  ],
  "license": "Apache-2.0",
  "scripts": {
    "build": "babel -q ./index.js -d ./dist && babel -q ./lib -d ./dist/lib && copy package.json dist",
    "postinstall": "npm run build",
    "lint": "standard",
    "test": "npm run lint && npm run unit",
    "unit": "NODE_ENV=test istanbul cover _mocha -- -R spec -b --recursive",
    "test-without-coverage": "NODE_ENV=test mocha -R spec -b --recursive"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bitfinexcom/bfx-hf-indicators.git"
  },
  "bugs": {
    "url": "https://github.com/bitfinexcom/bfx-hf-indicators/issues"
  },
  "keywords": [
    "honey framework",
    "bitfinex",
    "bitcoin",
    "BTC"
  ],
  "dependencies": {
    "babel-eslint": "^7.2.2",
    "babel-loader": "^6.4.1",
    "babel-plugin-lodash": "^3.2.11",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-regenerator": "^6.24.1",
    "babel-preset-env": "^1.4.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "bignumber.js": "^7.2.1",
    "copy": "^0.3.1",
    "lodash": "^4.17.10",
    "sprintf-js": "^1.1.1",
    "create-index": "^2.3.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^7.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^2.0.1",
    "istanbul": "^0.4.5",
    "mocha": "^6.2.0",
    "standard": "^14.2.0"
  }
}
