{
  "name": "bfx-hf-indicators",
  "version": "2.0.6",
  "description": "A library of trading indicators for Node.JS",
  "main": "./dist/index.js",
  "engines": {
    "node": ">=8.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn test && yarn build-docs && git add *.md"
    }
  },
  "directories": {
    "docs": "docs",
    "lib": "lib"
  },
  "author": "Bitfinex",
  "contributors": [
    "Cris Mihalache <cris.m@bitfinex.com> (https://www.bitfinex.com)",
    "Paolo Ardoino <paolo@bitfinex.com> (https://www.bitfinex.com)",
    "Jacob Plaster <jacob.plaster@bitfinex.com> (https://www.bitfinex.com)"
  ],
  "license": "Apache-2.0",
  "scripts": {
    "test": "yarn lint && yarn unit",
    "lint": "./node_modules/.bin/eslint '*.js' lib test examples",
    "unit": "NODE_ENV=test ./node_modules/.bin/nyc ./node_modules/.bin/mocha --recursive test",
    "update-deps": "./node_modules/.bin/updates -u -g -c",
    "update-version": "./node_modules/.bin/standard-version",
    "docs": "./node_modules/.bin/sermit .",
    "build-docs-api": "rm -rf DOCUMENTATION.md && ./node_modules/.bin/jsdoc2md -f index.js 'lib/*.js' > DOCUMENTATION.md",
    "build-docs-readme": "rm README.md && node scripts/gen_readme.js",
    "build-docs": "yarn build-docs-api && yarn build-docs-readme",
    "build-index": "./node_modules/.bin/babel ./index.js -d ./dist",
    "build-lib": "./node_modules/.bin/babel ./lib -d ./dist/lib",
    "build-meta": "./node_modules/.bin/copy package.json ./dist",
    "build": "yarn build-index && yarn build-lib && yarn build-meta && yarn build-docs",
    "prepublish": "yarn build",
    "release": "yarn build && yarn update-version && git push origin master --follow-tags"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bitfinexcom/bfx-hf-indicators.git"
  },
  "bugs": {
    "url": "https://github.com/bitfinexcom/bfx-hf-indicators/issues"
  },
  "keywords": [
    "honey framework",
    "bitfinex",
    "bitcoin",
    "BTC"
  ],
  "exports": {
    ".": "./index.js",
    "./lib": "./lib/index.js",
    "./package.json": "./package.json"
  },
  "dependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.5",
    "@babel/plugin-transform-regenerator": "^7.7.5",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-es2015": "^7.0.0-beta.53",
    "@babel/preset-stage-0": "^7.0.0",
    "babel-eslint": "^10.1.0",
    "bignumber.js": "^9.0.0",
    "copy": "^0.3.1",
    "create-index": "^2.3.0",
    "lodash": "^4.17.10",
    "sprintf-js": "^1.1.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "debug": "^4.1.1",
    "husky": "^4.2.3",
    "mocha": "^7.1.0",
    "nyc": "^15.0.0",
    "jsdoc-to-markdown": "^6.0.1",
    "eslint": "^7.2.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsdoc": "^27.0.4",
    "eslint-plugin-lodash": "^7.1.0",
    "eslint-plugin-mocha": "^7.0.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.3",
    "jsdoc-to-markdown": "^6.0.1",
    "markdown-it-anchor": "^7.0.0",
    "markdown-it-highlightjs": "^3.4.0",
    "markdown-it-smartarrows": "^1.0.1",
    "markdown-it-wikilinks": "^1.0.1",
    "mocha": "^7.1.0",
    "nyc": "^15.0.0",
    "serve-markdown-it": "^1.0.5",
    "serve-markdown-it-lib": "^1.0.5",
    "serve-markdown-it-template-blank": "^1.0.2",
    "serve-markdown-it-template-default": "^1.0.7",
    "standard": "^14.2.0",
    "standard-version": "^9.1.0",
    "updates": "^11.4.2",
    "webpack": "^5.11.1"
  }
}
